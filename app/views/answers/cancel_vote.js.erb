$('.flash').html('<%= j render partial: 'shared/flash' %>');
$('.cancel-vote-button').html('<%=
 if policy(Question).cancel_vote?
  link_to 'Cancel vote', cancel_vote_questions_path, class: 'btn btn-secondary my-2', method: :delete
 end %>');
$('.answers').html('<%= j render partial: 'answers/answer', collection: @question.answers.order_best %>')